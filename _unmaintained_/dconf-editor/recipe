pkg=dconf-editor
ver=3.20.1
rel=1
sum="dconf Editor"
lic=LGPL2.1
url=http://live.gnome.org/dconf
grp=gnome
dep=(gtk3 dconf)
mkd=(vala intltool)
src=(https://download.gnome.org/sources/$pkg/${ver:0:4}/$pkg-$ver.tar.xz)
sha=(7794f40b33dc376096896e10164b8571ab31ae1d50b9d0b40d409db2efaed75d)

build() {
    ./configure --prefix=/usr
    make
}

package() {
    make DESTDIR=$pkgdir install
}

pkg=dconf-editor
ver=3.22.1
rel=1
sum="dconf Editor"
lic=LGPL2.1
url=http://live.gnome.org/dconf
grp=gnome
dep=(gtk3 dconf)
mkd=(vala intltool)
src=(https://download.gnome.org/sources/$pkg/${ver:0:4}/$pkg-$ver.tar.xz)
sha=(0a0e56a5dc7cc20cd7127901069939f0c8049b585f90e229ef753ee9b3a4c126)

build() {
    ./configure --prefix=/usr
    make
}

package() {
    make DESTDIR=$pkgdir install
}

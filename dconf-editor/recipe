pkg=dconf-editor
ver=3.20.3
rel=1
sum="dconf Editor"
lic=LGPL2.1
url=http://live.gnome.org/dconf
grp=gnome
dep=(gtk3 dconf)
mkd=(vala intltool)
src=(https://download.gnome.org/sources/$pkg/${ver:0:4}/$pkg-$ver.tar.xz)
sha=(a8721499a277550b28d8dd94dafbea6efeb95fa153020da10603d0d4d628c579)

build() {
    ./configure --prefix=/usr
    make
}

package() {
    make DESTDIR=$pkgdir install
}
